{
    "name": "drmonty/datatables-fixedcolumns",
    "description": "FixedColumns Plugin for DataTables",
    "type": "component",
    "homepage": "http://www.datatables.net/",
    "author": {
        "name": "Allan Jardine",
        "url": "http://sprymedia.co.uk"
    },
    "require": {
        "robloach/component-installer": "*"
